<!DOCTYPE html>
<html>
  <head>
    <title><%=title %></title>
  </head>
<body>
  <h1><%=title %></h1>
  <br></br>
  <h2>Categories</h2>
  <% categories.map(category =>{ %>
   <form action ="/category" method ="get" >
     <input type="hidden" id="category" name="category" value="<%= category.id %>">
     <button type ="submit" ><%= category.category_name %></button>
   </form>
  <% }) %>
  <h2>Products</h2>

</body>
</html>